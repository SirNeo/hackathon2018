{
  "name": "hackathon2018",
  "version": "1.0.0",
  "description": "Proyecto javascript con webpack 3",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config=config/dev.webpack.config.js",
    "build": "rimraf dist && webpack --config=config/dev.webpack.config.js",
    "build:prod": "rimraf dist && webpack -p --config=config/prod.webpack.config.js",
    "lint": "npm run lint:html && npm run lint:xml",
    "lint:html": "eslint src -o ./eslint-report-manual.html -f html",
    "lint:xml": "eslint src -o ./eslint-report-manual.xml",
    "test": "karma start",
    "test:phantom": "karma start --single-run --browsers PhantomJS",
    "test:watch": "karma start --auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SirNeo/hackathon2018.git"
  },
  "author": "José María Palma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SirNeo/hackathon2018/issues"
  },
  "homepage": "https://github.com/SirNeo/hackathon2018#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^1.1.6",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.1",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.5.0",
    "karma-sinon-chai": "^1.3.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^2.0.9",
    "mocha": "^5.0.0",
    "node-sass": "^4.7.2",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.16",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "jquery": "^3.3.1",
    "materialize-css": "^0.100.2"
  }
}
